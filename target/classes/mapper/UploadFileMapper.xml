<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="exam.dao.UploadFileDao" >


    <insert id="insertUploadFile" parameterType="exam.bean.UploadFile" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO
        uploadFile (uploadFileName,uploadFileNameSrc)
        VALUES
        (#{uploadFileName},#{uploadFileNameSrc})
    </insert>

    <select id="queryUploadFile" parameterType="java.lang.Integer" resultType="exam.bean.UploadFile">
        select  * FROM uploadFile WHERE id=#{id}
    </select>

    <delete id="deleteUploadFile" parameterType="java.lang.Integer">

    </delete>


</mapper>